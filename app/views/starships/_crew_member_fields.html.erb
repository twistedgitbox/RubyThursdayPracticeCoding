<div class="crew-members">
      <%= f.label :name %>
      <%= f.text_field :name, class: "crew-name" %>
      <%= f.label :division %>
      <%= f.select(:division, ["Command", "Engineering", "Science"], {}, class: "crew-division") %>

<%= link_to_remove_association("Remove Crew Member", f, { wrapper_class: 'crew-members' }) %>
</div>
